{% extends 'base.html.twig' %}

{% block body %}
    <form class="form-signin" action="{{ 'login' }}" method="post">
        <label>Login</label>
        <input type="text" name="_username">
        <br>

        <label>Password</label>
        <input type="password" name="_password">
        <br>

        <button type="submit">Sign in</button>
    </form>
{% endblock %}